  "keywords": [
    "d3",
    "z-order",
    "morton",
    "space fill",
    "curve"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://github.com/vasturiano"
  },
  "license": "MIT",
  "type": "module",
  "jsdelivr": "dist/d3-morton.min.js",
  "unpkg": "dist/d3-morton.min.js",
  "main": "dist/d3-morton.mjs",
  "module": "dist/d3-morton.mjs",
  "exports": {
    "umd": "./dist/d3-morton.min.js",
    "default": "./dist/d3-morton.mjs"
  },
  "homepage": "https://github.com/vasturiano/d3-morton-order",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/d3-morton-order.git"
